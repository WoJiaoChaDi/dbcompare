<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chadi.dbcompare.dao.Dba_tablesMapper">


    <!--返回List-->
    <!-- public List<Employee> getEmpsByLastNameLike(String lastName); -->
    <!--resultType：如果返回的是一个集合，要写集合中元素的类型  -->
    <select id="getDba_tablesByOwner" resultType="com.chadi.dbcompare.bean.Dba_tables">
		SELECT t1.* FROM dba_tables t1 WHERE t1.OWNER = #{lastName}
    </select>

</mapper>